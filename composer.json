{
  "name": "pierrerolland/guzzle-config-operations-bundle",
  "description": "This bundle allows Symfony projects to add Guzzle operations to their configuration",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Pierre Rolland",
      "email": "roll.pierre@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "symfony/framework-bundle": "~4.0",
    "guzzlehttp/guzzle-services": ">=1.0",
    "doctrine/annotations": ">=1.2",
    "symfony/property-access": "~4.0"
  },
  "autoload": {
    "psr-0": { "Guzzle\\ConfigOperationsBundle": "" }
  },
  "target-dir": "Guzzle/ConfigOperationsBundle",
  "require-dev": {
    "phpspec/phpspec": ">=2.5.5",
    "symfony/property-info": "~4.0",
    "symfony/serializer": "~4.0"
  },
  "suggest": {
    "symfony/serializer": "~4.0"
  }
}
